<p>ここが投稿ページ<p>

<%= form_for @post do |f| %>
  <%= f.label :image, "Please post your taked Photo!!" %><br />

  <%= f.fields_for :images do |image_form| %><br />
    <%= image_form.file_field :image %><br />
  <% end %>

  <%= f.label :description, "Description" %><br />
  <%= f.text_area :description %><br />

  <%= f.submit "Please Post!!"%>
<% end %>
